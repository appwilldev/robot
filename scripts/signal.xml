<?xml version="1.0" encoding="UTF-8"?>
<roboto>
    <config>
		<url>http://chideat.org/media/index.html</url>
		<path>./data/</path>
        <ratio action='large'>
            <width>4000</width>
            <height>1384</height>
        </ratio>
    </config>
	<web id='root'>
		<target>
			<url>
			    <convert>
			        <tuple>
			            <from>chideat</from>
			            <to>chinat</to>
			        </tuple>
			        <tuple>
			            <from>.html</from>
			            <to>.jpg</to>
			        </tuple>
			        <tuple>
			            <from>/http/g</from>
			            <to>https</to>
			        </tuple>
			    </convert>
				<route>
					<div id='body'>
						<div>
							<ul id='list'>
								<li>
									<span>
										<a>[href]</a>
									</span>
								</li>
							</ul>
						</div>
					</div>
				</route>
			</url>
		</target>
	</web>
</roboto>
