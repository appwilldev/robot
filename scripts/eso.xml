<?xml version="1.0" encoding="UTF-8"?>
<roboto>
    <config>
		<url>http://www.eso.org/public/images/viewall/</url>
		<path>./data/</path>
        <ratio action='large'>
            <width>4000</width>
            <height>1384</height>
        </ratio>
    </config>
	<web id='root' child='sub'>
		<target>
			<url>
				<route>
					<div id='col3_content'>
						<div class='subcl'>
						    <table class='archive_normal'>
						        <td class='imagerow'>
						            <a>[href]</a>
						        </td>
						    </table>
						</div>
					</div>
				</route>
			</url>
		</target>
	</web>
    <web id='sub'>
		<target>
			<url>
				<route>
					<div id='col3_content'>
					    <div class='subcl'>
					        <div class='archive_image'>
					            <div class='image_container'>
					                <a>[href]</a>
	 				            </div>
                            </div>
                        </div>
					</div>
				</route>
			</url>
		</target>
	</web>
</roboto>
